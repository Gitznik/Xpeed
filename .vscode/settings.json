{
    "python.analysis.typeCheckingMode": "basic",
    "[python]": {
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
          "source.organizeImports": true
        },
        "editor.tabSize": 4,
      },
      "files.watcherExclude": {
        "**/target": true
      },
      "python.linting.pycodestyleEnabled": true,
}